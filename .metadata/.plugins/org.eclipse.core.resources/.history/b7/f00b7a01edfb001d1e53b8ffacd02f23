package laco_condicional;

import java.util.Scanner;

public class Exerciciofor {

	public static void main(String[] args) {
	Scanner in = new Scanner(System.in);
	
	int i, num1, num2;
	
	System.out.println("Insira 2 numeros");
	num1 = in.nextInt();
	num2 = in.nextInt();
	
	if(num1 < num2) {
		for(i = num1; i <= num2; i++) {
			
			if(i%3 == 0 || i%5 == 0) {
				System.out.println(i);
			}
		}
	}else {
		System.out.println("O primeiro Número não é menor que o segundo!");
	}
	
	

	}

}
